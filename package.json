    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "repository": {
    "type": "git",
